    "babel-loader": "6.3.1",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-preset-es2015": "6.22.0",
    "chai": "3.5.0",
    "codecov": "1.0.1",
    "eslint": "3.15.0",
    "eslint-config-standard": "6.2.1",
    "eslint-loader": "1.6.1",
    "eslint-plugin-promise": "3.4.1",
