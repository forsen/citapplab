{
  "name": "citapplab",
  "version": "0.6.6",
  "description": "A library for Citizen Science projects",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "npm run babel-prepublish && npm run flow-prepublish",
    "babel-prepublish": "babel src/ -d lib",
    "flow-prepublish": "flow-copy-source src lib",
    "test": "mocha --compilers js:babel-core/register --colors -w --recursive ./test/",
    "unit-test": "nyc --require babel-core/register mocha --recursive ./test/unit/",
    "integration-test": "mocha --compilers js:babel-core/register --colors --recursive ./test/",
    "report-coverage": "nyc report --reporter=lcov > coverage.lcov && codecov -t $CODECOV_API_KEY"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/forsen/citapplab.git"
  },
  "author": "Erik Haider Fors√©n <npm@mail.forsen.no>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/forsen/citapplab/issues"
  },
  "homepage": "https://github.com/forsen/citapplab#readme",
  "devDependencies": {
    "babel-core": "6.23.1",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.3.1",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-preset-es2015": "6.22.0",
    "chai": "3.5.0",
    "codecov": "1.0.1",
    "eslint": "3.15.0",
    "eslint-config-standard": "6.2.1",
    "eslint-loader": "1.6.1",
    "eslint-plugin-flowtype": "2.30.0",
    "eslint-plugin-promise": "3.4.1",
    "eslint-plugin-standard": "2.0.1",
    "flow-bin": "0.40.0",
    "flow-copy-source": "1.1.0",
    "mocha": "3.2.0",
    "nyc": "10.1.2"
  }
}
